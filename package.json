{
  "name": "simple-customizable-clock",
  "description": "A simple clock face with customizable colors and user activities.",
  "repository": "https://github.com/geoffhuntoon/simple-customizable-clock",
  "version": "1.1.0",
  "license": "MIT",
  "keywords": [
    "fitbit",
    "fitbit-dev",
    "sdk",
    "typescript"
  ],
  "fitbit": {
    "appUUID": "fb6a2f5f-8c1a-4307-8f28-f74152a910fe",
    "appType": "clockface",
    "appDisplayName": "Simple & Customizable Clock",
    "wipeColor": "#3f51b5",
    "requestedPermissions": [
      "access_activity",
      "access_heart_rate",
      "access_internet",
      "access_location",
      "access_user_profile"
    ],
    "buildTargets": [
      "meson",
      "gemini",
      "mira"
    ],
    "i18n": {
      "en": {
        "name": "Simple & Customizable Clock"
      }
    },
    "defaultLanguage": "en-US"
  },
  "devDependencies": {
    "@fitbit/sdk": "^4.1.0",
    "@fitbit/sdk-cli": "^1.7.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.3",
    "fitbit-sdk-types": "^5.0.0",
    "fitbit-weather": "^0.2.2",
    "fitbit-weather-types": "0.0.3",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.1"
  },
  "scripts": {
    "build": "npx fitbit-build",
    "test": "jest",
    "coverage": "jest --coverage"
  }
}
